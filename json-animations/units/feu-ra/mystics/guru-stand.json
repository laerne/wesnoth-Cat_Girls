{
    "$wml_root_path" : ":/images/units/feu-ra/mystics",
    "$gif_root_path" : ":/images-recolored/units/{color/}feu-ra/mystics",

    "$animations_to_export" : ["stand"],

    "$default_frame" : { "$duration" : 200 },

    "stand" : {
        "$gif_output_path" : ":/images-animated/units/{color/}feu-ra/mystics/guru{-$name}{-color}.gif",
        "$wml_output_path" : ":/animations/units/feu-ra/mystics/guru{-$name}.wml",
        "$frames" : [
            { "$input_path" : "guru{-color}.png" },
            { "$input_path" : "guru-stand-1{-color}.png" },
            { "$input_path" : "guru-stand-2{-color}.png" },
            { "$input_path" : "guru-stand-3{-color}.png" },
            { "$input_path" : "guru-stand-4{-color}.png" },
            { "$input_path" : "guru-stand-5{-color}.png" },
            { "$input_path" : "guru-stand-6{-color}.png" },
            { "$input_path" : "guru-stand-7{-color}.png" },
            { "$input_path" : "guru-stand-8{-color}.png" },
            { "$input_path" : "guru-stand-9{-color}.png" },
            { "$input_path" : "guru-stand-10{-color}.png" },
            { "$input_path" : "guru-stand-11{-color}.png" }
        ]
    },

    "$path_variables" : { "color" : "" },

    "$gif_path_variable_variations" : { "#include" : ":/json-includes/color-pairs.json" }
}
